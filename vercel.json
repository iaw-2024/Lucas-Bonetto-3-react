{
    "version": 2,
    "builds": [
        { "src": "api/index.js", "use": "@vercel/node" },
        { "src": "cliente/package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ]
}